{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "jsx": "react",
    "noEmit": true,
    "strict": true,
    "lib": ["dom", "dom.iterable", "es2017"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "Bundler",
    "skipLibCheck": true
  }
}
